<h3>{{t 'registries.overview.links.title'}}</h3>

{{#paginated-list/has-many
    modelTaskInstance=this.model.taskInstance
    relationshipName='linkedNodes'
    as |list|
}}
    {{#list.item as |child|}}
        <NodeCard @node={{child}} />
    {{/list.item}}
    {{#list.empty}}
        <p>{{t 'registries.overview.links.no_links'}}</p>
    {{/list.empty}}
{{/paginated-list/has-many}}
