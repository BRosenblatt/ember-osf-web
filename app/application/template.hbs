{{head-layout}}
{{title (t 'general.OSF')}}
<div local-class="Application">
    {{#unless disableHeader}}
        {{#if theme.isProvider}}
            <style>
                {{! template-lint-disable bare-strings }}
                @import url('{{theme.stylesheet}}');
                {{! template-lint-enable bare-strings }}
            </style>
            <div id="branded-navbar"></div>
        {{else}}
            {{osf-header}}
        {{/if}}
    {{/unless}}
    {{!--
    NOTE: the global Application__page class is used in registries
    to fix weird styling issues that break other embosf pages (quickfiles).
    --}}
    <div class="Application__page" local-class="Application__page">
        {{outlet}}
    </div>
    {{osf-footer}}
    {{cookie-banner}}
    {{osf-mode-footer}}
</div>
